<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from TemperatureWSService WSDL service -->

  <service name="TemperatureWS" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://ws.brewspberry.net"/>
      <parameter name="wsdlServiceElement" value="TemperatureWSService"/>
      <parameter name="schemaQualified" value="http://ws.brewspberry.net,http://beans.brewspberry.net"/>
      <parameter name="wsdlServicePort" value="TemperatureWS"/>
      <parameter name="className" value="net.brewspberry.ws.TemperatureWSSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="TemperatureWS"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getAllLastTemperature" qname="operNS:getAllLastTemperature" xmlns:operNS="http://ws.brewspberry.net" returnQName="retNS:getAllLastTemperatureReturn" xmlns:retNS="http://ws.brewspberry.net" returnType="rtns:ArrayOf_xsd_anyType" xmlns:rtns="http://ws.brewspberry.net" returnItemQName="tns:item" xmlns:tns="http://ws.brewspberry.net" soapAction="" >
      </operation>
      <operation name="getLastTemperature" qname="operNS:getLastTemperature" xmlns:operNS="http://ws.brewspberry.net" returnQName="retNS:getLastTemperatureReturn" xmlns:retNS="http://ws.brewspberry.net" returnType="rtns:Temperature" xmlns:rtns="http://beans.brewspberry.net" soapAction="" >
        <parameter qname="pns:probe" xmlns:pns="http://ws.brewspberry.net" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="returnLastLineFromCSV" qname="operNS:returnLastLineFromCSV" xmlns:operNS="http://ws.brewspberry.net" returnQName="retNS:returnLastLineFromCSVReturn" xmlns:retNS="http://ws.brewspberry.net" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="getLastTemperature getAllLastTemperature returnLastLineFromCSV"/>

      <typeMapping
        xmlns:ns="http://ws.brewspberry.net"
        qname="ns:>getAllLastTemperature"
        type="java:net.brewspberry.ws.GetAllLastTemperature"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://ws.brewspberry.net"
        qname="ns:>getLastTemperatureResponse"
        type="java:net.brewspberry.ws.GetLastTemperatureResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://ws.brewspberry.net"
        qname="ns:>returnLastLineFromCSV"
        type="java:net.brewspberry.ws.ReturnLastLineFromCSV"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://ws.brewspberry.net"
        qname="ns:>getAllLastTemperatureResponse"
        type="java:net.brewspberry.ws.GetAllLastTemperatureResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://beans.brewspberry.net"
        qname="ns:Temperature"
        type="java:net.brewspberry.beans.Temperature"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <arrayMapping
        xmlns:ns="http://ws.brewspberry.net"
        qname="ns:ArrayOf_xsd_anyType"
        type="java:java.lang.Object[]"
        innerType="cmp-ns:anyType" xmlns:cmp-ns="http://www.w3.org/2001/XMLSchema"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://ws.brewspberry.net"
        qname="ns:>returnLastLineFromCSVResponse"
        type="java:net.brewspberry.ws.ReturnLastLineFromCSVResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://ws.brewspberry.net"
        qname="ns:>getLastTemperature"
        type="java:net.brewspberry.ws.GetLastTemperature"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
